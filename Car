package com.olenick.programmingCourse;

public class Car extends Vehicle {
    protected static final Double getWidth() { return 3.0; }
    protected static final Double getHeight() { return 1.5; }
    protected static final Double getLength() { return 4.5; }
    protected static final String getTypeAsString() { return "car"; }

    public static Dimensions getDimensions() {
        return new Dimensions(getWidth(), getHeight(), getLength());
    }

    protected final String name;

    public Car(String name) {
        this.name = name;
    }

    protected String getName() { return name; }
}
